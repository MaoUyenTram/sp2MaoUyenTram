@model IEnumerable<Website.Models.QAnswers>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Answers.Answer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Questions.Question)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Answers.Answer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Questions.Question)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Users.Email)
        </td>
    </tr>
}

</table>
